   "source": [
    "#calculate the count of even numbers upto a given number n\n",
    "n= int(input(\"enter the number: \"))\n",
    "count_even= 0\n",
    "for i in range(1, n+1):\n",
    "    if i %2==0:\n",
    "        count_even+= 1\n",
    "print(\"count of even number is: \", count_even)\n"
   ]
